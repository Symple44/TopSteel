{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": false,
    "incremental": false,
    "tsBuildInfoFile": null,
    "emitDeclarationOnly": true,
    "noEmit": false,
    "outDir": "./dist"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "../domains/src/**/*.ts"],
  "exclude": ["node_modules", "dist", "**/*.test.ts", "**/*.spec.ts", "**/*.stories.tsx"]
}
