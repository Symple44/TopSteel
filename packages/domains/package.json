{
  "name": "@erp/domains",
  "version": "1.0.0",
  "description": "Domaines métiers modularisés pour TopSteel ERP",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./sales": {
      "types": "./dist/sales/index.d.ts",
      "import": "./dist/sales/index.js",
      "require": "./dist/sales/index.cjs"
    },
    "./production": {
      "types": "./dist/production/index.d.ts",
      "import": "./dist/production/index.js",
      "require": "./dist/production/index.cjs"
    },
    "./cross-cutting": {
      "types": "./dist/cross-cutting/index.d.ts",
      "import": "./dist/cross-cutting/index.js",
      "require": "./dist/cross-cutting/index.cjs"
    },
    "./search": {
      "types": "./dist/search/index.d.ts",
      "import": "./dist/search/index.js",
      "require": "./dist/search/index.cjs"
    },
    "./image": {
      "types": "./dist/image/index.d.ts",
      "import": "./dist/image/index.js",
      "require": "./dist/image/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "sharp": "^0.33.5",
    "@elastic/elasticsearch": "^8.15.0",
    "@elastic/transport": "^8.7.0",
    "apache-arrow": "^18.1.0",
    "debug": "^4.4.1",
    "secure-json-parse": "^3.0.1",
    "ms": "^2.1.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "hpagent": "^1.2.0",
    "undici": "^6.21.3",
    "tslib": "^2.8.1",
    "flatbuffers": "^24.3.25",
    "color": "^4.2.3",
    "color-string": "^1.9.1",
    "color-name": "^1.1.4",
    "color-convert": "^2.0.1",
    "simple-swizzle": "^0.2.2",
    "is-arrayish": "^0.3.2",
    "semver": "^7.6.3",
    "detect-libc": "^2.0.3"
  },
  "devDependencies": {
    "@erp/config": "workspace:*",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.4",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}