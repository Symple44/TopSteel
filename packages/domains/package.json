{
  "name": "@erp/domains",
  "version": "1.0.0",
  "description": "Domaines métiers modularisés pour TopSteel ERP",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./sales": {
      "types": "./dist/sales/index.d.ts",
      "import": "./dist/sales/index.js",
      "require": "./dist/sales/index.cjs"
    },
    "./production": {
      "types": "./dist/production/index.d.ts",
      "import": "./dist/production/index.js",
      "require": "./dist/production/index.cjs"
    },
    "./cross-cutting": {
      "types": "./dist/cross-cutting/index.d.ts",
      "import": "./dist/cross-cutting/index.js",
      "require": "./dist/cross-cutting/index.cjs"
    },
    "./search": {
      "types": "./dist/search/index.d.ts",
      "import": "./dist/search/index.js",
      "require": "./dist/search/index.cjs"
    },
    "./image": {
      "types": "./dist/image/index.d.ts",
      "import": "./dist/image/index.js",
      "require": "./dist/image/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "sharp": "^0.34.3",
    "@elastic/elasticsearch": "^9.0.3",
    "@elastic/transport": "^9.1.0",
    "apache-arrow": "^21.0.0",
    "debug": "^4.4.1",
    "secure-json-parse": "^4.0.0",
    "ms": "^2.1.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "hpagent": "^1.2.0",
    "undici": "^7.11.0",
    "tslib": "^2.8.1",
    "flatbuffers": "^25.2.10",
    "color": "^5.0.0",
    "color-string": "^2.0.1",
    "color-name": "^2.0.0",
    "color-convert": "^3.1.0",
    "simple-swizzle": "^0.2.2",
    "is-arrayish": "^0.3.2",
    "semver": "^7.6.3",
    "detect-libc": "^2.0.3"
  },
  "devDependencies": {
    "@erp/config": "workspace:*",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}