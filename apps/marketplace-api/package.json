{
  "name": "@erp/marketplace-api",
  "version": "1.0.0",
  "description": "API Marketplace multi-tenant pour TopSteel ERP",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "npm run typeorm -- migration:generate -d src/infrastructure/database/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/infrastructure/database/data-source.ts",
    "migration:run:direct": "ts-node -r tsconfig-paths/register src/scripts/run-migration.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/infrastructure/database/data-source.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@erp/entities": "workspace:*",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.4",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.1",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "redis": "^5.8.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.27",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@erp/config": "workspace:*",
    "@erp/types": "workspace:*",
    "@erp/utils": "workspace:*",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=8.15.0"
  },
  "keywords": [
    "nestjs",
    "marketplace",
    "e-commerce",
    "multi-tenant",
    "erp-integration"
  ],
  "author": {
    "name": "TopSteel",
    "email": "dev@topsteel.fr"
  },
  "license": "GPL-3.0"
}
