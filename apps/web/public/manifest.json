  "shortcuts": [
    {
      "name": "View Products",
      "short_name": "Products",
      "description": "Browse our product catalog",
      "url": "/marketplace/products",
      "icons": [
        {
          "src": "/icons/shortcuts/products.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "My Orders",
      "short_name": "Orders",
      "description": "View your order history",
      "url": "/marketplace/account/orders",
      "icons": [
        {
          "src": "/icons/shortcuts/orders.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Shopping Cart",
      "short_name": "Cart",
      "description": "View your shopping cart",
      "url": "/marketplace/cart",
      "icons": [
        {
          "src": "/icons/shortcuts/cart.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.topsteel.marketplace",
      "id": "com.topsteel.marketplace"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/topsteel-marketplace/id123456789"
    }
  ],
  "prefer_related_applications": false,
  "lang": "en-US",
  "dir": "ltr",
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["image/*", "application/pdf"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+topsteel",
      "url": "/marketplace/products?id=%s"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "browser"
  ],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "file_handlers": [
    {
      "action": "/open",
      "accept": {
        "application/pdf": [".pdf"],
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls", ".xlsx"]
      }
    }
  ]
}