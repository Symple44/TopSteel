{
  "scripts": {
    "prebuild": "biome check src/ --diagnostic-level=warn || echo Continue malgre warnings",
    "build:check": "biome check src/ && next build",
    "build:fix": "biome check --apply src/ && next build",
    "build": "next build",
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=development next dev -p 3005",
    "dev:original": "cross-env NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=development next dev -p 3005",
    "ws-server": "node websocket-server.js",
    "start": "next start -p 3005",
    "lint": "biome check src/",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:windows": "cross-env NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=production NEXT_BUILD_STANDALONE=false next build",
    "clean": "rimraf .next out dist",
    "clean:cache": "node clean-cache.js",
    "postinstall": "next telemetry disable",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint:biome:fix": "biome check --apply src/",
    "format:biome": "biome format --write src/",
    "lint:biome": "biome check src/"
  },
