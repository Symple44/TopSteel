{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "tsc",
    "typeCheck": false,
    "tsConfigPath": "tsconfig.json",
    "assets": ["**/*.proto", "**/*.graphql"],
    "watchAssets": true
  },
  "projects": {},
  "generateOptions": {
    "spec": false
  }
}
